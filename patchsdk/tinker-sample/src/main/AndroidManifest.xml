<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dx168.patchsdk.sample">

    <application
        android:name="com.dx168.patchsdk.sample.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dx168.patchsdk.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service
            android:name=".tinker.TinkerResultService"
            android:exported="false"/>

        <service
            android:name=".tinker.SamplePatchService"
            android:exported="false"
            android:process=":patch"/>
    </application>

</manifest>